<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox id="container" fx:id="container" alignment="CENTER"
	prefHeight="500.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<HBox alignment="TOP_RIGHT" prefHeight="100.0" prefWidth="200.0">
			<children>
				<Label id="score" text="Score: " />
				<Label id="score_value" text="0000" />
			</children>
			<VBox.margin>
				<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
			</VBox.margin>
		</HBox>
		<GridPane id="board" fx:id="board" alignment="CENTER"
			disable="true" gridLinesVisible="true" maxHeight="-Infinity"
			maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
			prefHeight="300.0" prefWidth="300.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="333.0"
					minWidth="10.0" prefWidth="300.0" />
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="480.0"
					minWidth="10.0" prefWidth="300.0" />
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="480.0"
					minWidth="10.0" prefWidth="300.0" />
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="480.0"
					minWidth="10.0" prefWidth="300.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
			</rowConstraints>
		</GridPane>
		<GridPane alignment="CENTER" prefHeight="200.0">
			<columnConstraints>
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="333.0"
					minWidth="10.0" prefWidth="300.0" />
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="480.0"
					minWidth="10.0" prefWidth="300.0" />
				<ColumnConstraints hgrow="SOMETIMES" maxWidth="480.0"
					minWidth="10.0" prefWidth="300.0" />
			</columnConstraints>
			<rowConstraints>
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
				<RowConstraints minHeight="10.0" prefHeight="30.0"
					vgrow="SOMETIMES" />
			</rowConstraints>
			<children>
				<Button id="btn_top" alignment="CENTER" contentDisplay="CENTER"
					maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
					mnemonicParsing="false" text="Top" GridPane.columnIndex="1" />
				<Button id="btn_left" alignment="CENTER" contentDisplay="CENTER"
					maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
					mnemonicParsing="false" text="Left" GridPane.rowIndex="1" />
				<Button id="btn_down" alignment="CENTER" contentDisplay="CENTER"
					maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
					mnemonicParsing="false" text="Donw" GridPane.columnIndex="1"
					GridPane.rowIndex="2" />
				<Button id="btn_right" alignment="CENTER" contentDisplay="CENTER"
					maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
					mnemonicParsing="false" text="Right" GridPane.columnIndex="2"
					GridPane.rowIndex="1" />
			</children>
		</GridPane>
	</children>
</VBox>
